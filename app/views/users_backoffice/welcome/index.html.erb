<div class="row">
  <div class="col-lg-12">
      <h1 class="page-header">Linsting Users</h1>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th>ID</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
              <td><%= user.id %></td>
              <td><%= user.email %></td>
          </tr>
          <% end %>
      </tbody>
  </table>
</div>
